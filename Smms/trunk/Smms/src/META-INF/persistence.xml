<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="SmmsPU" transaction-type="JTA">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<jta-data-source>java:comp/env/jdbc/mysmmsDS</jta-data-source>
		<class>biz.evolix.model.Users</class>
		<class>biz.evolix.model.Sku</class>
		<class>biz.evolix.model.Purchese</class>
		<class>biz.evolix.model.Province</class>
		<class>biz.evolix.model.Order</class>
		<class>biz.evolix.model.NodeDescription</class>
		<class>biz.evolix.model.Node1</class>
		<class>biz.evolix.model.Brance</class>
		<class>biz.evolix.model.Authorities</class>
		<class>biz.evolix.model.BonusPeriod</class>
		<properties>
			<property name="eclipselink.logging.level" value="FINEST" />
			<property name="eclipselink.cache.type.default" value="Weak"/>			
		</properties>
	</persistence-unit>
</persistence>
